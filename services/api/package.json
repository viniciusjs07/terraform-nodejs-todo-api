{
  "name": "api",
  "version": "1.0.0",
  "description": "A RESTfull API for managing todo lists, built with Express, TypeScript, and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "start:dev": "tsx --watch src/index.ts",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts --minify",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts"
  },
  "keywords": [
    "express",
    "typescript",
    "postgres",
    "drizzle",
    "api"
  ],
  "author": "Vinicus Silva",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "pg": "^8.12.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/k6": "^0.53.1",
    "@types/node": "^22.5.0",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.24.2",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}
